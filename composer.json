{
  "name": "dragosprotung/stc-core",
  "description": "Core libraries for sport trackers",
  "keywords": [
    "sport tracker connector"
  ],
  "homepage": "https://github.com/dragosprotung/stc-core",
  "license": "MIT",
  "authors": [
    {
      "name": "Dragos Protung",
      "email": "dragos@protung.ro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~7.0",
    "psr/log": "^1.0",
    "league/flysystem": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "squizlabs/php_codesniffer": "^2.7",
    "phpmd/phpmd": "^2.4",
    "sebastian/phpcpd": "^2.0",
    "scrutinizer/ocular": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "SportTrackerConnector\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SportTrackerConnector\\Core\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
